{
  "expo": {
    "name": "project-parking",
    "slug": "project-parking",
    "version": "1.0.0",
    "icon": "./app/assets/png/Logo.png",
    "scheme": "meuapp",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/png/Logo.png",
        "backgroundColor": "#FFFFFF"  
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ooliveira.tg.projectparking"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/png/Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D0D0D"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c952c3d4-0ba3-48ed-ab73-94b7f29e263e"
      }
    }
  }
}
