{
  "expo": {
    "name": "project-parking",
    "slug": "project-parking",
    "version": "1.0.0",
    "icon": "./app/assets/png/Logo.png",
    "scheme": "meuapp",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/png/Logo.png",
        "backgroundColor": "#0D0D0D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ooliveira.tg.projectparking",
      "splash": {
        "image": "./app/assets/png/Logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0D0D0D"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/png/Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D0D0D"
        }
      ],
      "expo-font"
    ],
    "splash": {
      "image": "./app/assets/png/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d9b85507-4725-4067-b069-0c65ff71e6dd"
      }
    }
  }
}

 